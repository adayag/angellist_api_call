	<h3>New Startup</h3>
			<%= form_for @startup, url: {action: "fill_from_angel_list"}, html: {method: 'GET', :class => "pure-form pure-form-aligned"} do |f| %>
	  	 <div class="pure-control-group">
			  <%=f.label :angel_list_url%>
			  <%=f.text_field :angel_list_url, :required => true, :type => 'url' %>
			  <button class="pure-button">Fill from AngelList</button>
			 </div>
			 <div class="pure-control-group">
			  <%=f.label :name%>
			  <%=f.text_field :name %>
			 </div>

			 <div class="pure-control-group">
			  <%=f.label :city %>
			  <%=f.text_field :city%>
			 </div>

			 <div class="pure-control-group">
			  <%=f.label :logo%>
			  <%=f.text_field :logo_url%>
			 </div>
				  <div class="img">
				  	<img src= <%="#{@startup.logo_url}"%>>
				  </div>
			 <div class="pure-control-group">
			  <%=f.label :product_description%>
			  <%=f.text_area :product_description, cols: 20, rows:10%>
			 </div>

			 <div class="pure-control-group">
			  <%=f.label :high_concept_pitch%>
			  <%=f.text_area :high_concept_pitch, cols: 20, rows:10%>
			 </div>

			 <div class="pure-control-group">
			  <%=f.label "Market Names"%>
			  <%=f.text_area :market_names, cols: 20, rows: 10%>
	     </div>
			<%end%>

